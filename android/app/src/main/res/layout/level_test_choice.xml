<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_alignWithParentIfMissing="false"
    android:layout_centerHorizontal="false"
    android:background="@drawable/level_test_choice" >

    <Button
        android:id="@+id/delete_button"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="859dp"
        android:layout_marginTop="59dp"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="482dp"
        android:background="@android:drawable/ic_delete"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/seed_level"
        android:layout_width="466dp"
        android:layout_height="84dp"
        android:layout_marginStart="288dp"
        android:layout_marginTop="71dp"
        android:layout_marginEnd="105dp"
        android:background="#00FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/delete_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/plant_level"
        android:layout_width="466dp"
        android:layout_height="78dp"
        android:layout_marginStart="288dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="206dp"
        android:background="#00FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seed_level" />

    <Button
        android:id="@+id/flower_level"
        android:layout_width="462dp"
        android:layout_height="76dp"
        android:layout_marginStart="284dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="214dp"
        android:background="#00FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/plant_level" />

    <Button
        android:id="@+id/level_test_button"
        android:layout_width="437dp"
        android:layout_height="106dp"
        android:layout_marginStart="275dp"
        android:layout_marginTop="73dp"
        android:layout_marginEnd="248dp"
        android:layout_marginBottom="68dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/flower_level"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>